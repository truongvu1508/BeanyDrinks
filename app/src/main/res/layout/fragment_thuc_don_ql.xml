<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".thucDonQl">

    <EditText
        android:id="@+id/editTextText6"
        android:layout_width="320dp"
        android:layout_height="53dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="5dp"
        android:background="@drawable/edittext_border_timkiem"
        android:drawableRight="@drawable/search"
        android:drawablePadding="8dp"
        android:ems="10"
        android:hint="Tìm kiếm"
        android:inputType="text"
        android:paddingLeft="12dp"
        android:paddingRight="40dp"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.629"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Chọn theo loại món"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.105"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText6" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="202dp"
        android:layout_height="58dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/rounded_spinner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.246"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <View
        android:id="@+id/roundedRectangleView"
        android:layout_width="335dp"
        android:layout_height="110dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/rounded_rectangle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner" />

    <View
        android:id="@+id/roundedRectangleView2"
        android:layout_width="335dp"
        android:layout_height="110dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/rounded_rectangle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/roundedRectangleView" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="96dp"
        android:layout_height="111dp"
        android:layout_marginStart="8dp"
        android:src="@drawable/__ca_phe_sua_1586320543_3124"
        app:layout_constraintBottom_toBottomOf="@+id/roundedRectangleView"
        app:layout_constraintStart_toStartOf="@+id/roundedRectangleView"
        app:layout_constraintTop_toTopOf="@+id/roundedRectangleView"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Cà phê sữa"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/roundedRectangleView"
        app:layout_constraintHorizontal_bias="0.119"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/roundedRectangleView" />


    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Cà phê muối"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/roundedRectangleView2"
        app:layout_constraintHorizontal_bias="0.101"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/roundedRectangleView2" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="72dp"
        android:adjustViewBounds="true"
        android:background="#FFDDDDDD"
        android:contentDescription="Chỉnh sửa"
        android:scaleType="fitCenter"
        android:src="@drawable/_64599"
        app:layout_constraintEnd_toEndOf="@+id/roundedRectangleView2"
        app:layout_constraintTop_toTopOf="@+id/roundedRectangleView2" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="72dp"
        android:adjustViewBounds="true"
        android:background="#FFDDDDDD"
        android:contentDescription="Chỉnh sửa"
        android:scaleType="fitCenter"
        android:src="@drawable/_64599"
        app:layout_constraintEnd_toEndOf="@+id/roundedRectangleView"
        app:layout_constraintTop_toTopOf="@+id/roundedRectangleView" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="4dp"
        android:adjustViewBounds="true"
        android:background="#FFDDDDDD"
        android:contentDescription="Chỉnh sửa"
        android:scaleType="fitCenter"
        android:src="@drawable/_861362"
        app:layout_constraintEnd_toEndOf="@+id/roundedRectangleView"
        app:layout_constraintTop_toTopOf="@+id/roundedRectangleView" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="4dp"
        android:adjustViewBounds="true"
        android:background="#FFDDDDDD"
        android:contentDescription="Chỉnh sửa"
        android:scaleType="fitCenter"
        android:src="@drawable/_861362"
        app:layout_constraintEnd_toEndOf="@+id/roundedRectangleView"
        app:layout_constraintTop_toTopOf="@+id/roundedRectangleView2" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="25.000 đ"
        app:layout_constraintEnd_toEndOf="@+id/roundedRectangleView"
        app:layout_constraintHorizontal_bias="0.086"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="22.000 đ"
        app:layout_constraintEnd_toEndOf="@+id/roundedRectangleView"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="96dp"
        android:layout_height="111dp"
        android:src="@drawable/citi_44689_2"
        app:layout_constraintEnd_toEndOf="@+id/roundedRectangleView2"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="@+id/roundedRectangleView2"
        app:layout_constraintTop_toTopOf="@+id/roundedRectangleView2" />

    <Button
        android:id="@+id/button4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/button_circle"
        android:text="+"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/roundedRectangleView2" />


</androidx.constraintlayout.widget.ConstraintLayout>
